{
  "name": "wtako-rest-template",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "watch": [
      "app/src/*"
    ]
  },
  "scripts": {
    "start": "sh -c 'cd app/dist && node bin/server.js'",
    "clean": "rm -rf dist && mkdir dist",
    "dev": "npm run build-watch & sleep 5 && nodemon --exec sh -c 'npm start'",
    "prod": "npm run build && npm start",
    "build": "cd app && babel src --out-dir dist --copy-files",
    "build-watch": "cd app && babel src --out-dir dist --copy-files --watch"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "@hapi/joi": "^17.1.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "bull": "^3.22.4",
    "cheerio": "^1.0.0-rc.6",
    "cjk-count": "^1.0.3",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-cluster": "0.0.5",
    "firebase-admin": "^9.9.0",
    "form-data": "^4.0.0",
    "ioredis": "^4.27.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.19",
    "knex-types": "^0.2.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mz": "^2.7.0",
    "object-assign-deep": "^0.4.0",
    "pg": "^8.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",

    "@babel/core": "^7.14.8",
    "@babel/cli": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/plugin-transform-flow-strip-types": "7.14.5",

    "babel-eslint": "^8.2.6",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.5.1"
  }
}
