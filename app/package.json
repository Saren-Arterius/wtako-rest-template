{
  "name": "wtako-rest-template",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "watch": [
      "app/src/*"
    ]
  },
  "scripts": {
    "start": "sh -c 'cd app/dist && node bin/server.js'",
    "clean": "rm -rf dist && mkdir dist",
    "dev": "yarn build-watch & sleep 5 && nodemon --exec sh -c 'yarn start'",
    "prod": "yarn build && yarn start",
    "build": "babel app/src --out-dir app/dist --copy-files",
    "build-watch": "babel app/src --out-dir app/dist --copy-files --watch"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bull": "^3.4.4",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.3",
    "firebase-admin": "^5.13.1",
    "ioredis": "^3.2.2",
    "knex": "^0.15.2",
    "morgan": "^1.9.0",
    "mz": "^2.7.0",
    "pg": "^7.4.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/knex": "^0.14.17",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.5.1"
  }
}
